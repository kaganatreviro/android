    <style name="disable_button" parent="Widget.AppCompat.Button">
        <item name="android:background">@color/disable_button</item>
        <item name="android:textColor">@color/disable_text_color</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">18sp</item>
        <item name="cornerRadius">12dp</item>
    </style>
    <style name="empty_text_view" parent="Widget.AppCompat.TextView">
